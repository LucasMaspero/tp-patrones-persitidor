package persistidor.comandos;

import persistidor.excepciones.NoExisteSesionException;

public interface IObtenerObjetoDeClaseAsociadoASesionComando
{
	/**
	* Elimina una clase por ID y sus atributos asociados.
	* @param idClase ID de la clase a eliminar. 
	*/
	<T> T ejecutar(long idSesion, Class<T> clazz) throws NoExisteSesionException;
}
