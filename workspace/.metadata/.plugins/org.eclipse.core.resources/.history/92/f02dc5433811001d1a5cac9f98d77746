package persistidor.comandos;

public interface IVerificarSiClaseEsUnTipoValidoComando
{
	/**
     * Verifica si la clase es un tipo valido para persistir indirectamente.
     * @param clase Clase a verificar.
     * @param permitirColeccion booleano indicando si coleccion es un tipo valido.
    **/
	boolean ejecutar(Class<?> clase, boolean permitirColeccion);
}
